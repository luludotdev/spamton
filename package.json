{
  "name": "spamton",
  "packageManager": "yarn@3.2.1",
  "type": "module",
  "main": "./build/index.js",
  "private": true,
  "scripts": {
    "lint": "prettier --check . && eslint src --ext mjs,js,ts",
    "format": "prettier --write . && eslint src --ext mjs,js,ts --fix",
    "fmt": "yarn run format",
    "clean": "rimraf build",
    "compile": "ttsc",
    "check": "yarn run compile --noEmit",
    "build": "yarn run clean && yarn run compile",
    "test": "yarn run check && yarn run lint",
    "dev": "yarn run clean && yarn run compile --watch",
    "debug": "nodemon --watch build --inspect"
  },
  "engines": {
    "node": ">=16.9.0"
  },
  "devDependencies": {
    "@types/node": "^16",
    "eslint": "^8.23.1",
    "eslint-config-neon": "^0.1.33",
    "nodemon": "^2.0.16",
    "prettier": "^2.7.1",
    "rimraf": "^3.0.2",
    "ttypescript": "^1.5.13",
    "typescript": "^4.7.3",
    "typescript-transform-paths": "^3.3.1"
  },
  "dependencies": {
    "@discordx/importer": "^1.1.9",
    "@discordx/utilities": "^4.3.3",
    "@lolpants/env": "^0.4.0",
    "@lolpants/exit": "^0.1.0",
    "@lolpants/jogger": "^0.6.0",
    "discord.js": "^13.8.0",
    "discordx": "^9.7.2",
    "execa": "^6.1.0",
    "reflect-metadata": "^0.1.13",
    "source-map-support": "^0.5.21",
    "tslib": "^2.4.0"
  }
}
