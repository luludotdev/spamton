{
  "name": "spamton",
  "packageManager": "yarn@3.5.0",
  "type": "module",
  "main": "./build/index.js",
  "private": true,
  "scripts": {
    "lint": "prettier --check . && eslint src --ext mjs,js,ts",
    "format": "prettier --write . && eslint src --ext mjs,js,ts --fix",
    "fmt": "yarn run format",
    "clean": "rimraf build",
    "compile": "ttsc",
    "check": "yarn run compile --noEmit",
    "build": "yarn run clean && yarn run compile",
    "test": "yarn run check && yarn run lint",
    "dev": "yarn run clean && yarn run compile --watch",
    "debug": "nodemon --watch build --inspect"
  },
  "engines": {
    "node": ">=16.9.0"
  },
  "devDependencies": {
    "@lolpants/eslint-config": "^0.4.0",
    "@types/node": "^18.15.11",
    "eslint": "^8.37.0",
    "nodemon": "^2.0.22",
    "prettier": "^2.8.7",
    "rimraf": "^4.4.1",
    "ttypescript": "^1.5.15",
    "typescript": "^4.9.5",
    "typescript-transform-paths": "^3.4.6"
  },
  "dependencies": {
    "@discordx/importer": "^1.2.1",
    "@lolpants/env": "^0.6.2",
    "@lolpants/exit": "^0.2.0",
    "@lolpants/jogger": "^0.8.1",
    "discord.js": "^14.9.0",
    "discordx": "^11.7.4",
    "execa": "^7.1.1",
    "reflect-metadata": "^0.1.13",
    "source-map-support": "^0.5.21",
    "tslib": "^2.5.0"
  }
}
