{
  "name": "spamton",
  "type": "module",
  "main": "./dist/index.js",
  "private": true,
  "scripts": {
    "lint": "prettier --check . && eslint src",
    "format": "prettier --write . && eslint src --fix",
    "fmt": "pnpm run format",
    "check": "tsc --noEmit --skipLibCheck",
    "test": "pnpm run check && pnpm run lint",
    "build": "tsdown",
    "dev": "pnpm run build --watch ./src",
    "debug": "nodemon --watch dist --inspect"
  },
  "devDependencies": {
    "@luludev/eslint-config": "^0.8.0",
    "@types/node": "^24.9.1",
    "eslint": "^9.38.0",
    "nodemon": "^3.1.10",
    "prettier": "^3.6.2",
    "rimraf": "^6.0.1",
    "tsdown": "^0.15.9",
    "typescript": "^5.9.3"
  },
  "dependencies": {
    "@luludev/env": "^0.6.6",
    "@luludev/exit": "^0.2.2",
    "@luludev/jogger": "^0.9.6",
    "discord.js": "^14.24.0",
    "discordx": "^11.13.2",
    "execa": "^9.6.0",
    "reflect-metadata": "^0.2.2",
    "ts3-nodejs-library": "^3.5.1"
  },
  "pnpm": {
    "overrides": {
      "@stylistic/eslint-plugin": "^5",
      "@stylistic/eslint-plugin-jsx": "npm:@stylistic/eslint-plugin@^5",
      "@stylistic/eslint-plugin-ts": "npm:@stylistic/eslint-plugin@^5",
      "@angular-eslint/eslint-plugin": "-",
      "@angular-eslint/eslint-plugin-template": "-",
      "@angular-eslint/template-parser": "-",
      "astro-eslint-parser": "-",
      "eslint-plugin-astro": "-",
      "eslint-plugin-cypress": "-",
      "eslint-plugin-rxjs": "-",
      "eslint-plugin-rxjs-angular": "-",
      "eslint-plugin-svelte3": "-",
      "eslint-plugin-vue": "-",
      "vue-eslint-parser": "-"
    },
    "onlyBuiltDependencies": [
      "cpu-features",
      "ssh2",
      "unrs-resolver"
    ]
  }
}
